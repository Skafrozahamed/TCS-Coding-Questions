#include <iostream>
using namespace std;
int main()
{
    int num,temp,sum=0;
    cout<<"Enter a number : ";
    cin>>num;
    temp=num;
 for (int i = 1; i <= num; i++)
    {
        if (num % i == 0)
        {
            sum = (sum+i);
        }
    }
    sum=sum-num;
    
    if (temp<sum)
    {
        cout<<temp<<" is an abundant number .";
    }
    else
    {
       cout<<temp<<" is not an abundant number ."; 
    }

    return 0;
}
