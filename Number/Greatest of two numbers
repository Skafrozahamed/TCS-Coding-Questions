#include<iostream>
using namespace std;
int main() {
  double num1, num2;
  cout<<"Enter first number : ";
  cin>>num1;
  cout<<"Enter second number : ";
  cin>>num2;
	if (num1 < num2) {
		cout <<"The greatest of the two numbers is "<<num2;
	}
	else {
		cout <<"The greatest of the two numbers is " <<num1;
	}
  return 0;
}
