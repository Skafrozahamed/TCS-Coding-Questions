#include <iostream>

using namespace std;

int main()
{
int num1 , num2 ;
cout<< " Take two number : ";
cin>> num1>> num2;
	int ans;
	for (int i = 1,j=1; i <= num1, j<=num2; i++,j++) {
		if (num1 % i == 0 && num2 % j == 0) {
			ans = i=j;
		}
	}
	cout << "The GCD of the two numbers is "<<ans;
    return 0;
}
