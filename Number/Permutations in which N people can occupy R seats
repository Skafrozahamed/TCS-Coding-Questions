#include <iostream>
using namespace std;
int main()
{
  int n,r,fact1=1,fact2=1,Permutation=0;
  cout<<"N : ";
  cin>> n;
  cout<<"R : ";
  cin>> r;
  
  for (int i=1;i<=n;i++)
  {
      fact1*=i;
      
  }
  
  for (int j=1;j<=(n-r);j++)
  {
      fact2*=j;
      
  }
  
  Permutation=fact1/fact2;
  cout<< Permutation;
    return 0;
}
