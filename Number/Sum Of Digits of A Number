#include <iostream>

using namespace std;

int main()
{
  int num,sum=0;
  cout<< "Enter a number : ";
  cin>> num;
  int temp = num;

     while (num > 0) {
      
      int count = num % 10;
      
      sum = sum + count;
      
      num = num / 10;
   }
	cout << "The sum of the  number's digit is "<<sum;
    return 0;
}
